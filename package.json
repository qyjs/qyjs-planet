  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel serve:portal serve:app1 serve:app2",
    "serve:portal": "ng run portal:serve",
    "serve:app1": "ng serve app1 --servePath=/app1/ --configuration development",
    "serve:app2": "ng serve app2 --servePath=/app2/ --configuration development",
    "dev:app1": "ng serve app1",
    "build:portal": "ng build",
    "build:app1": "ng build app1",
    "build:app2": "ng build app2",
    "// build:app1:dev": "ng build app1 --watch true --baseHref=/app1/",
    "// build:app2:dev": "ng build app2 --watch true --baseHref=/app2/",
    "build:portal:prod": "ng build --configuration production",
    "build:app1:prod": "ng build app1 --configuration production --baseHref=/app1/",
    "build:app2:prod": "ng build app2 --configuration production --baseHref=/app2/",
    "build": "ng build planet",
    "build:demo": "npm run build:app1 && npm run build:app2 && npm run build:portal",
    "build:demo:prod": "npm run build:app1:prod && npm run build:app2:prod && npm run build:portal:prod",
    "test": "ng test planet",
    "lint": "ng lint planet"
  },
