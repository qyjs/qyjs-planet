{
  "name": "qyjs-planet",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "micro front-end",
    "micro frontend",
    "microfrontend",
    "micro frontends",
    "angular micro front-end",
    "ngx micro front-end"
